class C{
    var x;
    var y;
    C(){
	self.x = 0;
	self.y = 0;
    }
    
    function getItem(n){
	switch(n){
	    case 0:
		return self.x;
	    case 1:
		return self.y;
	    default:
		return null;
	}
    }
    
    function setItem(n, v){
	switch(n){
	    case 0:
		self.x = v;
		break;
	    case 1:
		self.y = v;
		break;
	}
    }
}


var out;

var c = new C();

(c[0] += 2) *= 5;
c[1] = 2;
++c[1];

out = c[1];
